    "dependencies": {
        "@auth0/auth0-spa-js": "^1.7.0",
        "concurrently": "^5.1.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-jwt": "^5.3.3",
        "if-env": "^1.0.4",
        "jwks-rsa": "^1.8.0",
        "mongoose": "^5.9.9",
        "npm-run-all": "^4.1.5",
        "react-router-dom": "^5.1.2",
        "socket.io": "^2.2.0"
    }
}
